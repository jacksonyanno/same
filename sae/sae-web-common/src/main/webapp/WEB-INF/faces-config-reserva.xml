<?xml version="1.0" encoding="UTF-8"?>
<faces-config version="2.0" xmlns="http://java.sun.com/xml/ns/javaee"
 xmlns:xi="http://www.w3.org/2001/XInclude"
 xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-facesconfig_2_0.xsd">
 <managed-bean>
  <managed-bean-name>utilMBean</managed-bean-name>
  <managed-bean-class>uy.gub.imm.sae.web.mbean.UtilMBean</managed-bean-class>
  <managed-bean-scope>request</managed-bean-scope>
 </managed-bean>
 <managed-bean>
  <managed-bean-name>dummyMBean</managed-bean-name>
  <managed-bean-class>uy.gub.imm.sae.web.common.DummyMBean</managed-bean-class>
  <managed-bean-scope>request</managed-bean-scope>
 </managed-bean>
 <managed-bean>
  <managed-bean-name>coreMBean</managed-bean-name>
  <managed-bean-class>uy.gub.imm.sae.web.common.CoreMBean</managed-bean-class>
  <managed-bean-scope>request</managed-bean-scope>
 </managed-bean>
 <managed-bean>
  <managed-bean-name>sesionMBean</managed-bean-name>
  <managed-bean-class>uy.gub.imm.sae.web.mbean.reserva.SesionMBean</managed-bean-class>
  <managed-bean-scope>session</managed-bean-scope>
 </managed-bean>
 <managed-bean>
  <managed-bean-name>paso1MBean</managed-bean-name>
  <managed-bean-class>uy.gub.imm.sae.web.mbean.reserva.Paso1MBean</managed-bean-class>
  <managed-bean-scope>request</managed-bean-scope>
  <managed-property>
   <property-name>sesionMBean</property-name>
   <property-class>uy.gub.imm.sae.web.mbean.reserva.SesionMBean</property-class>
   <value>#{sesionMBean}</value>
  </managed-property>
 </managed-bean>
 <managed-bean>
  <managed-bean-name>paso2MBean</managed-bean-name>
  <managed-bean-class>uy.gub.imm.sae.web.mbean.reserva.Paso2MBean</managed-bean-class>
  <managed-bean-scope>request</managed-bean-scope>
  <managed-property>
   <property-name>sesionMBean</property-name>
   <property-class>uy.gub.imm.sae.web.mbean.reserva.SesionMBean</property-class>
   <value>#{sesionMBean}</value>
  </managed-property>
 </managed-bean>
 <managed-bean>
  <managed-bean-name>paso3MBean</managed-bean-name>
  <managed-bean-class>uy.gub.imm.sae.web.mbean.reserva.Paso3MBean</managed-bean-class>
  <managed-bean-scope>request</managed-bean-scope>
  <managed-property>
   <property-name>sesionMBean</property-name>
   <property-class>uy.gub.imm.sae.web.mbean.reserva.SesionMBean</property-class>
   <value>#{sesionMBean}</value>
  </managed-property>
  <managed-property>
   <property-name>datosReservaMBean</property-name>
   <property-class>java.util.Map</property-class>
   <value>#{datosReservaMBean}</value>
  </managed-property>
 </managed-bean>
 <managed-bean>
  <managed-bean-name>datosReservaMBean</managed-bean-name>
  <managed-bean-class>java.util.HashMap</managed-bean-class>
  <managed-bean-scope>request</managed-bean-scope>
 </managed-bean>
 <managed-bean>
  <managed-bean-name>pasoFinalMBean</managed-bean-name>
  <managed-bean-class>uy.gub.imm.sae.web.mbean.reserva.PasoFinalMBean</managed-bean-class>
  <managed-bean-scope>request</managed-bean-scope>
  <managed-property>
   <property-name>sesionMBean</property-name>
   <property-class>uy.gub.imm.sae.web.mbean.reserva.SesionMBean</property-class>
   <value>#{sesionMBean}</value>
  </managed-property>
 </managed-bean>
 <managed-bean>
  <managed-bean-name>kaptchaMBean</managed-bean-name>
  <managed-bean-class>uy.gub.imm.sae.web.mbean.reserva.KaptchaMBean</managed-bean-class>
  <managed-bean-scope>request</managed-bean-scope>
 </managed-bean>
 <navigation-rule>
  <from-view-id>/agendarReserva/Paso1.xhtml</from-view-id>
  <navigation-case>
   <from-outcome>siguientePaso</from-outcome>
   <to-view-id>/agendarReserva/Paso2.xhtml</to-view-id>
   <redirect/>
  </navigation-case>
 </navigation-rule>
 <navigation-rule>
  <from-view-id>/agendarReserva/Paso2.xhtml</from-view-id>
  <navigation-case>
   <from-outcome>siguientePaso</from-outcome>
   <to-view-id>/agendarReserva/Paso3.xhtml</to-view-id>
   <redirect/>
  </navigation-case>
 </navigation-rule>
 <navigation-rule>
  <from-view-id>/agendarReserva/Paso3.xhtml</from-view-id>
  <navigation-case>
   <from-outcome>reservaConfirmada</from-outcome>
   <to-view-id>/agendarReserva/PasoFinal.xhtml</to-view-id>
   <redirect/>
  </navigation-case>
  <navigation-case>
   <from-outcome>pasoAnterior</from-outcome>
   <to-view-id>/agendarReserva/Paso2.xhtml</to-view-id>
   <redirect/>
  </navigation-case>
 </navigation-rule>
 <navigation-rule>
  <navigation-case>
   <from-outcome>inicio</from-outcome>
   <to-view-id>/agendarReserva/Paso1.xhtml</to-view-id>
   <redirect/>
  </navigation-case>
  <navigation-case>
   <from-outcome>agendaInvalida</from-outcome>
   <to-view-id>/error/agendaInvalida.xhtml</to-view-id>
   <redirect/>
  </navigation-case>
  <navigation-case>
   <from-outcome>estadoInvalido</from-outcome>
   <to-view-id>/error/estadoInvalido.xhtml</to-view-id>
   <redirect/>
  </navigation-case>
  <navigation-case>
   <from-outcome>periodoInvalido</from-outcome>
   <to-view-id>/error/periodoInvalido.xhtml</to-view-id>
   <redirect/>
  </navigation-case>
 </navigation-rule>
</faces-config>

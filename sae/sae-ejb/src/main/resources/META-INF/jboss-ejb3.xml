<?xml version="1.0" encoding="UTF-8"?>
<jboss:ejb-jar xmlns="http://java.sun.com/xml/ns/javaee"
	xmlns:jboss="http://www.jboss.com/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns:s="urn:security"
	version="3.1" impl-version="2.0">

	<assembly-descriptor>

		<s:security>
			<ejb-name>*</ejb-name>
			<!-- Name of the security domain which is configured in the EJB3 subsystem -->
			<s:security-domain>${same.build.security.domain.jndi}</s:security-domain>
		</s:security>

	</assembly-descriptor>

</jboss:ejb-jar>

